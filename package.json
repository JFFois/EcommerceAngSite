{
  "name": "front-bee-paris",
  "version": "1.0.0",
  "description": "A bee project",
  "scripts": {
    "webpack-dev": "cross-env NODE_ENV=development webpack --mode development",
    "webpack-prod": "cross-env NODE_ENV=production webpack --mode production",
    "build:dev": "webpack-dev-server --inline --hot --progress --port 8080",
    "build:development": "npm run build:clean && ngc && npm run webpack-prod && npm run build:clean",
    "build:staging": "npm run build:clean && ngc && npm run webpack-prod && npm run build:clean",
    "build:production": "npm run build:clean && ngc && npm run webpack-prod && npm run build:clean",
    "build:clean": "del-cli 'src/**/*.js' 'src/**/*.js.map' 'src/**/*.ngsummary.json' 'src/**/*.metadata.json' 'src/**/**/*.ngfactory.ts' 'src/**/*.ngstyle.ts' 'src/**/*.shim.ts'",
    "serve": "lite-server",
    "server:prod:ci": "http-server dist -p 3000 -c-1 --cors"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "~10.1.1",
    "@angular/cdk": "~10.2.0",
    "@angular/common": "~10.1.1",
    "@angular/compiler": "~10.1.1",
    "@angular/compiler-cli": "~10.1.1",
    "@angular/core": "~10.1.1",
    "@angular/forms": "~10.1.1",
    "@angular/http": "~7.2.16",
    "@angular/platform-browser": "~10.1.1",
    "@angular/platform-browser-dynamic": "~10.1.1",
    "@angular/platform-server": "~10.1.1",
    "@angular/router": "~10.1.1",
    "@angular/upgrade": "~10.1.1",
    "@ngx-translate/core": "~12.1.2",
    "angular2-uuid": "~1.1.1",
    "core-js": "~2.6",
    "font-awesome": "~4.7.0",
    "json2typescript": "~1.4.1",
    "primeflex": "~2.0.0",
    "primeicons": "~4.0.0",
    "primeng": "~10.0.0",
    "rxjs": "~6.6.3",
    "rxjs-compat": "~6.6.3",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@ngtools/webpack": "~7.3",
    "@types/core-js": "~2.5",
    "@types/node": "~10.12",
    "angular-router-loader": "~0.8.5",
    "angular2-template-loader": "~0.6.2",
    "awesome-typescript-loader": "~5.2.1",
    "babel-core": "~6.26.3",
    "babel-loader": "~8.1",
    "babel-preset-env": "~1.7",
    "clean-webpack-plugin": "~0.1",
    "codelyzer": "~6.0.0",
    "copy-webpack-plugin": "^6.1.1",
    "cross-env": "~7.0.2",
    "css-loader": "~4.3.0",
    "cssnano": "~4.1.10",
    "del-cli": "~3.0.1",
    "file-loader": "~6.1.0",
    "html-loader": "~1.3.0",
    "html-webpack-plugin": "~3.2",
    "lite-server": "~2.4",
    "node-sass": "~4.14.1",
    "optimize-css-assets-webpack-plugin": "~5.0",
    "optimize-js-plugin": "0.0.4",
    "sass-loader": "~10.0.2",
    "style-loader": "~1.2.1",
    "terser-webpack-plugin": "~4.2.1",
    "to-string-loader": "~1.1.6",
    "tslint": "~6.1.3",
    "tslint-import-group-ordering": "~2.1.2",
    "typescript": "~4.0.2",
    "webpack": "~4.44.1",
    "webpack-cli": "~3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-filter-warnings-plugin": "~1.2.1",
    "webpack-merge": "~4.2"
